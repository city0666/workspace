<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="liusir.test.listviewtest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="liusir.test.listviewtest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity 
            android:name="liusir.test.listviewtest.ArrayAdapterActivity"
            android:label="@string/look_arraylistview" >
        </activity>
        
        <activity 
            android:name="liusir.test.listviewtest.MyListView"
            android:label="@string/look_mylistview" >
        </activity>
        
        <activity
            android:name="liusir.test.listviewtest.MySimpleAdapter"
            android:label="@string/look_simpleadapter" >
        </activity>
       
        <activity 
            android:name="liusir.test.listviewtest.DisplayContact"></activity>
        <activity android:name="liusir.test.listviewtest.SimpleCursorAdapterActivity"></activity>
        <activity android:name="liusir.test.listviewtest.ContactsActivity"></activity>
        
        
    </application>

</manifest>